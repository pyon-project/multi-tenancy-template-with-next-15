{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:store": "cp .env.store .env.local && next dev --turbopack --port 3001",
    "dev:board": "cp .env.board .env.local && next dev --turbopack --port 3002",
    "dev:trade": "cp .env.trade .env.local && next dev --turbopack --port 3003",
    "build": "next build",
    "build:store": "cp .env.store .env.local && NEXT_PUBLIC_APP_NAME=Store next build",
    "build:board": "cp .env.board .env.local && NEXT_PUBLIC_APP_NAME=Board next build",
    "build:trade": "cp .env.trade .env.local && NEXT_PUBLIC_APP_NAME=Trade next build",
    "start": "next start",
    "start:store": "NEXT_PUBLIC_APP_NAME=Store next start --port 3001",
    "start:board": "NEXT_PUBLIC_APP_NAME=Board next start --port 3002",
    "start:trade": "NEXT_PUBLIC_APP_NAME=Trade next start --port 3003",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "prepare": "husky",
    "test": "jest",
    "create-site": "tsx create.ts"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "inquirer": "^12.4.2",
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^3.2.3",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-plugin-storybook": "^0.11.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
